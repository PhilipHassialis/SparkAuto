@page
@model SparkAuto.Pages.ServiceTypes.CreateModel
@{
    ViewData["Title"] = "Create";
}

<br />
<h2 class="text-info">Create Service Type</h2>
<br />
<form method="post">
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="ServiceType.Name"></label>
        </div>
        <div class="col-5">
            <input asp-for="ServiceType.Name" style="width:100%" />
            <span asp-validation-for="ServiceType.Name" class="text-danger" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="ServiceType.Price"></label>
        </div>
        <div class="col-5">
            <input asp-for="ServiceType.Price" style="width:100%" />
            <span asp-validation-for="ServiceType.Price" class="text-danger" />
        </div>
    </div>
    <div class="form-group row">
        <div class="offset-2 col-5">
            <partial name="_CreateAndBackToListButton" />
        </div>
    </div>
</form>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}


