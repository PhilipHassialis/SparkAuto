@page
@model SparkAuto.Pages.ServiceTypes.CreateModel
@{
    ViewData["Title"]="Create";
}

<br/>
<h2 class="text-info">Create Service Type</h2>
<br/>
<form method="post">
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="ServiceType.Name"></label>
        </div>
        <div class="col-5">
            <input asp-for="ServiceType.Name" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="ServiceType.Price"></label>
        </div>
        <div class="col-5">
            <input asp-for="ServiceType.Price" />
        </div>
    </div>
    <div class="form-group row">
        <div class="offset-2 col-5">
            <div class="row">
                <div class="col-6">
                    <input type="submit" class="btn btn-info form-control" value="Create" />
    
                </div>
                <div class="col-6">
                    <a asp-action="Index" class="btn btn-success form-control">Back To List</a>
                </div>
            </div>
        </div>
        </div>
</form>



